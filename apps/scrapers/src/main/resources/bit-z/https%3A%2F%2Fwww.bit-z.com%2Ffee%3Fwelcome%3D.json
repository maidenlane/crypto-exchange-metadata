<script type="text/javascript">
function BitzRedirect(url) {
        var reg = /javascript|script|document|cookie|eval|setTimeout|alert/;
        if (reg.test(url)) {
          return;
        }

        if (/^\//.test(url)) {
          url = location.protocol+"//"+location.host + url;
        }
        if (url == location.href) {
          return;
        }
        if (document.referrer == url) {
          return;
        }

        location.href = url;
      };

      window.onerror = function (m, u, r, c, e) {
        var stk = '';
        if (e && e.stack) {
          stk = (e.stack || e.stacktrace).toString();
        }
        sa_sendLog("js_error", {
          msg: m,
          url: u,
          r: r,
          c: c || "not know",
          stack: stk
        });
      };

      window.APP_CONFIG = {
        ENV: "prod",
        LANG: "zh",
        XTOKEN: "",
        'BZ-SessionId': "",

        API_DOMAIN: "https://ucapi.bit-z.pro",
