        function scrollExe(hash) {

            var targetTop = 0;

            if (hash.length === 0) {
                return false;
            }

            targetTop = $(hash).offset().top - $headerNav.outerHeight() * 1.2;

            if (navigator.userAgent.indexOf('iPhone') > -1){
                targetTop -= iOSTabBarHeight;
            }

            $('body, html').animate({
                "scrollTop": (targetTop)
            }, 'fast');
        }

        //bootstrap の dropdown とのバッティングを回避するために、ターゲットを data() 化する
        $anchor.each(function (_, v) {
            $(v).data('href', $(v).attr('href'));
        });
        $headerNav.find('.dropdown').children('a').attr('href', '#');

        $headerNav.before($spacer);

        $w.scroll(function () {

            if ($w.scrollTop() >= headerNavTop) {
                $headerNav.addClass('pos-fixed'),
                $spacer.height(headerNavHeight);
            } else {
                //$headerNav.removeClass('pos-fixed');
                $headerNav.removeClass('pos-fixed'),
                $spacer.height(0);
            }

        });

        $w.load(function () {

            var target = window.location.hash;

            scrollExe(target);

            return false;

        });

        $anchor.click(function () {

            var $this = $(this),
                href = $this.data('href'),
                targetTop = 0;

            if (!$this.parent().hasClass('dropdown') || $this.parent().hasClass('open')) {
                scrollExe(href);
                location.href = href;
            }

        });

        //SP用プルダウン

        function getChildNav($lis) {
            var parentText = "",
                pt = "";
            $lis.each(function (_,li) {
                var $li = $(li),
                    $_potion;
                if ($li.parent().parent().hasClass('dropdown') === false) {
                    parentText = pt = "";
                }
                if ($li.hasClass('dropdown') && $li.find('li').length > 0) {
                    parentText = $li.children('a').text();
                } else {
                    pt = (parentText !== "") ? parentText + " - " : "";
                    $_option = $('<option />').val($li.children('a').attr('href'));
                    $_option.text(pt + $li.children('a').text());
                    $_select.append($_option);
                }
            });
        }
        getChildNav( $PC_Nav.find('li'))
        $SP_Nav.append($_select);
        $_select.change(function () {
            scrollExe( $(this).val());
        });

    });

</script>


